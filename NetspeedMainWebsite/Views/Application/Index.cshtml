    <script>
        //for id-card-info
        $('input[name="firstname"]').on("change", function () {
            var firstname = $(this).val();
            var lastname = $('input[name="lastname"]').val();
            var val = firstname + " " + lastname;
            $('#id-card-info').text("Kimlik Bilgileriniz (" + val + ")");
        });
        $('input[name="lastname"]').on("change", function () {
            var lastname = $(this).val();
            var firstname = $('input[name="firstname"]').val();
            var val = firstname + " " + lastname;
            $('#id-card-info').text("Kimlik Bilgileriniz (" + val + ")");
        });
        $('input').on("input", function () {
            if (!$(this).attr("name").includes("mail")) {
                $(this).val($(this).val().toLocaleUpperCase());
            }
        });
        function ClearValidations(selector) {
            selector.removeClass("is-invalid");
            selector.parent("div").find(".fv-plugins-message-container").remove();
        }
        ChangeIDCardIssueDateType();
        function ChangeIDCardIssueDateType() {
            if ($('#idCardType').val() == 2) {
                $('select[name="kimlikyil"]').prev('label').text("Kimlik Veriliş Tarihi (Yıl)");
                $('select[name="kimlikay"]').prev('label').text("Kimlik Veriliş Tarihi (Ay)");
                $('select[name="kimlikgun"]').prev('label').text("Kimlik Veriliş Tarihi (Gün)");

                $('input[name="kimlikil"]').parent("div").parent("div").show();
                $('input[name="kimlikilce"]').parent("div").parent("div").show();
                $('input[name="kimlikmahalle"]').parent("div").parent("div").show();
                $('input[name="ailesirano"]').parent("div").parent("div").show();
                $('input[name="sirano"]').parent("div").parent("div").show();
                $('input[name="ciltno"]').parent("div").parent("div").show();
            } else {
                $('input[name="kimlikil"]').parent("div").parent("div").hide();
                $('input[name="kimlikilce"]').parent("div").parent("div").hide();
                $('input[name="kimlikmahalle"]').parent("div").parent("div").hide();
                $('input[name="ailesirano"]').parent("div").parent("div").hide();
                $('input[name="sirano"]').parent("div").parent("div").hide();
                $('input[name="ciltno"]').parent("div").parent("div").hide();

                $('select[name="kimlikyil"]').prev('label').text("Kimlik Geçerlilik Tarihi (Yıl)");
                $('select[name="kimlikay"]').prev('label').text("Kimlik Geçerlilik Tarihi (Ay)");
                $('select[name="kimlikgun"]').prev('label').text("Kimlik Geçerlilik Tarihi (Gün)");
            }
        }
        $('#idCardType').change(function () {
            ChangeIDCardIssueDateType();
        });
        //check idcard dates
        $('select[name="kimlikyil"]').change(function () {
            CheckDate("kimlikyil", "kimlikay", "kimlikgun");
        });
        $('select[name="kimlikay"]').change(function () {
            CheckDate("kimlikyil", "kimlikay", "kimlikgun");
        });
        $('select[name="kimlikgun"]').change(function () {
            CheckDate("kimlikyil", "kimlikay", "kimlikgun");
        });
        //check birth dates
        $('select[name="dogumgun"]').change(function () {
            CheckDate("dogumyili", "dogumay", "dogumgun");
        });
        $('select[name="dogumay"]').change(function () {
            CheckDate("dogumyili", "dogumay", "dogumgun");
        });
        $('select[name="dogumyili"]').change(function () {
            CheckDate("dogumyili", "dogumay", "dogumgun");
        });
        function CheckDate(yearName, monthName, dayName) {
            var gun = $('select[name="' + dayName + '"]').val();
            var ay = $('select[name="' + monthName + '"]').val();
            var yil = $('select[name="' + yearName + '"]').val();
            if (gun && ay && yil) {
                $.ajax({
                    url: '@Url.Action("DateTimeValidaiton", "Application")',
                    method: 'POST',
                    data: { year: yil, month: ay, day: gun },
                    complete: function (data, status) {
                        if (status == "success") {
                            var result = data.responseJSON;
                            if (!result) {
                                $('select[name="' + dayName + '"]').val("");
                                $('select[name="' + monthName + '"]').val("");
                                $('select[name="' + yearName + '"]').val("");
                                ShowErrorMessage("Geçersiz tarih formatı. Lütfen geçerli bir tarih seçiniz.");
                            }
                        }
                    }
                });
            }
        }
    </script>
    <script>
        $('#sehir').change(function () {
            GetValues('#sehir', '#ilce', '@Url.Action("GetDistricts", "InfrastructureInquiry")');
        })
        $('#ilce').change(function () {
            GetValues('#ilce', '#semt','@Url.Action("GetRegions", "InfrastructureInquiry")');
        })
        $('#semt').change(function () {
            GetValues('#semt', '#mahalle', '@Url.Action("GetNeighborhoods", "InfrastructureInquiry")');
        })
        $('#mahalle').change(function () {
            GetValues('#mahalle', '#sokak','@Url.Action("GetStreets", "InfrastructureInquiry")');
        })
        $('#sokak').change(function () {
            GetValues('#sokak', '#aptbina', '@Url.Action("GetBuildings", "InfrastructureInquiry")');
        })
        $('#aptbina').change(function () {
            GetValues('#aptbina', '#kapino', '@Url.Action("GetApartments", "InfrastructureInquiry")');
        })
        $('#kapino').change(function () {
            $.ajax({
                url: '@Url.Action("GetAddressText", "Application")',
                data: { apartmentCode: $(this).val() },
                method: 'POST',
                complete: function (data, status) {
                    if (status == "success") {
                        $("#address-text").text(data.responseText);
                    }
                    else {
                        $("#address-text").text("-");
                    }
                }
            })
        })
    </script>
